### hooks/env

ARCHIVE_TAG="ubuntu18.04.3"
TODAY=$(date +%Y-%m-%d)
BASETAG=""

case "$DOCKER_TAG" in

    latest | dev)
        BASETAG="latest"
        SUPPORT_USER_GROUP_OVERRIDE=
        ;;

    lab | dev-lab)
        BASETAG="latest"
        SUPPORT_USER_GROUP_OVERRIDE="yes"
        ;;

    *)
        BASETAG=""
        ;;
esac
